{
    "name": "akkurate/laravel-media",
    "description": "Media package for Laravel applications",
    "type": "library",
    "require": {
        "akkurate/laravel-core": "^2.0",
        "akkurate/laravel-back-components": "^2.0",
        "akkurate/laravel-search": "^2.0",
        "intervention/image": "^2.5",
        "laravel/legacy-factories": "^1.1",
        "spatie/laravel-medialibrary": "^8.3.0",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-tags": "^2.6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0"
    },
    "autoload" : {
        "psr-4": {
            "Akkurate\\LaravelMedia\\": "src",
            "Akkurate\\LaravelMedia\\Database\\Seeders\\": "database/seeders",
            "Akkurate\\LaravelMedia\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Akkurate\\LaravelMedia\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akkurate\\LaravelMedia\\LaravelMediaServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-access.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-admin.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-auth.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-back-components.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-business.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-carbon-icons.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-contact.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-core.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-me.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:subvitaminetm/laravel-search.git"
        }
    ]
}
